
<div class="">

<div class="row">

<div class="col-sm-4">
<div class="fancy">

<%= form_for(:partner_preference, url: search_result_match_index_path, method: :get) do |form| %>
     <div class="form-inline">
     <label>Age Range</label>
  <div class="col-sm-12">
   <%= form.select :age_from, options_for_select(@age_list.map {|p| [  p[:name],p[:value] ] },:selected=>params[:partner_preference].presence && params[:partner_preference][:age_from]),{include_blank:true},class:"form-control col-sm-3"%>
  <%= form.label :To,class:"label-control col-sm-3" %>
  <%= form.select :age_to,  options_for_select(@age_list.map {|p| [  p[:name],p[:value] ] },:selected=>params[:partner_preference].presence && params[:partner_preference][:age_to] ),{include_blank:true},class:"form-control col-sm-3" %>
      </div>
    </div>
  <div class="form-inline">
 <label>Height</label>
          <div class="col-sm-12">
            <%= form.text_field :height_from,  class:"col-sm-3"%> 
            <%= form.label :To,class:"label-control col-sm-3" %>
            <%= form.text_field :height_to, class:"col-sm-3"%> 
          </div>
  </div>
     <div class="form-group">
      <div class="control-label row">
        <%= form.label :marital_status,"Marital Status" %>
      </div>
      <div class="row">
        <%= form.select(:marital_status , options_for_select(MaritalStatus.all.collect{|p| [ p.name, p.name ] }, params[:partner_preference].try(:[], :marital_status)),  { include_blank: true },class:"form-control",id: "marital_status") %>
      </div>
    </div>
   <div class="form-group">
     <div class="control-label row">
        <%= form.label :have_children,"Have Children" %>
      </div>
      <div class="row">
        <%= form.select(:have_children,options_for_select([['Yes', 'Yes'],['No', 'No']], params[:partner_preference].presence && params[:partner_preference][:have_children]),  { include_blank: true },class:"form-control",id: "have_children") %>
      </div>
  </div>
   <div class="form-group">
     <div class="control-label row">
        <%= form.label :religion,"Religion" %>
      </div>
      <div class="row">
        <%= form.select(:religion,options_for_select(Religion.all.collect {|p| [ p.name, p.name ] }, params[:partner_preference].presence &&  params[:partner_preference][:religion]), { include_blank: true },class:"form-control",id: "religion") %>
      </div>
  </div>
   <div class="form-group">
     <div class="control-label row">
        <%= form.label :community,"Community" %>
      </div>
      <div class="row">
      <%= form.text_field :community,value:'brahmin', class:'form-control'%>
        
      </div>
  </div>
   <div class="form-group">
    <div class="control-label row">
        <%= form.label :education_level,"Education Level" %>
      </div>
      <div class="row">
        <%= form.select(:education_level,options_for_select([['No formal education', 'No formal education'],['Primary education', 'Primary education'],['Secondary education','Secondary education'],['GED','GED'],["Vocational qualification","Vocational qualification"],["Bachelor's degree","Bachelor's degree"],["Master's degree","Master's degree"],['Doctorate or higher','Doctorate or higher']], params[:partner_preference].presence && params[:partner_preference][:education_level]),  { include_blank: true },class:"form-control",id: "education_level") %>
      </div>
  </div>
    <div class="form-group">
      <div class="control-label row">
        <%= form.label :job_status %>
      </div>
      <div class="row">
        <%= form.select(:job_status,options_for_select(Jobstatus.all.collect {|p| [ p.name, p.name ] },params[:partner_preference].presence && params[:partner_preference][:job_status]),  { include_blank: true },class:"form-control",id: "user_info_job_status") %>
      </div>
    </div>
    <div class="form-group">
    <div class="row">
    <input type="button" class="btn btn-primary" onClick="patnerSearchGird()" value="Search Patner" >
    </div>
    </div>
  <% end %>    
</div>
 </div>


<div class="col-sm-8">
 
<div class="profile_grid" style=" padding: 25px;">
</div>

</div>
</div>


</div>

</div>

<!-- Modal -->
<div id="modelSendConnection" class="modal fade in" role="dialog">
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close-model close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title" style="float:left">Send a message</h4>
      </div>
      <div class="modal-body">
        <input id="userMessage" class="form-control" placeholder="Introduce yourself to Member Name in a few sentences. Start by typing here.">
 
        </input>
      </div>
      <div class="modal-footer">
      <button type="button" class="btn btn-primary connect-profile" onClick="sendRequest()">Send Message</button>
        <button type="button" class="btn btn-default close-model" data-dismiss="modal">Close</button>
      </div>
    </div>

  </div>
</div>


